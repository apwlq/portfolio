<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>My Projects</title>
</head>
<body>
  <h1>My Projects</h1>
  <% for (const data of datas) { %>
    <div class="data">
      <h2><%= data.title %></h2>
      <img src="<%= data.imageUrl %>" alt="<%= data.title %>">
      <p><%= data.description %></p>
      <a href="<%= data.url %>">View project</a>
    </div>
  <% } %>
</body>
</html>
